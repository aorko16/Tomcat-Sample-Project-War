
pipeline {
    agent {
        label 'agent-1'
    }

    tools {
        maven 'maven-3.6.1'
        jdk 'jdk-17'
    }

    stages {

        stage('Compile') {
            steps {
                sh 'mvn compile'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn package'
            }
        }

    }
}
