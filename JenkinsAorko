pipeline {
    agent {
        label 'slave-1'
    }

    stages {

        stage('Compile-1') {
            steps {
                sh 'mvn compile'
            }
        }

        stage('Test-2') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Build-3') {
            steps {
                sh 'mvn package'
            }
        }

    }
}
